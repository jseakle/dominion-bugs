javascript:fetch("https://tipr.tk/static/dombugs.json").then(e=>e.json()).then(e=>{let t=document.getElementsByClassName("name-layer"),l=[],n=[];for(let s of t)s.children[0]&&l.push(s.children[0].innerHTML);let o=[];for(let f of e.bugs){if("Rare"==f.at(-1))continue;let i=0;for(let u of f)if("!"==u[0]){for(let r of e[u])if(l.includes(r)&&!o.includes(r)){i+=1;break}}else l.includes(u)&&(i+=1,o.push(u));i!=f.length-1||n.includes(f.at(-1))||n.push(f.at(-1))}if(n.length){console.log(n);let a="";for(let h of n)a+=h+"\n\n";alert(a+"\nIf you disprove OR REPRODUCE any of these bugs, please let us know in #shuffleit-client!")}});
